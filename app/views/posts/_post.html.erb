<tr>
  <td><%= post.title %></td>
  <td><%= post.body %></td>
  <td><%= post.author %></td>
  <td><%= post.created_at.strftime("%d %b, %y") %></td>
  <td><%= post.updated_at.strftime("%d %b, %y") %></td>
  <td><%= link_to "Editar", edit_post_path(post) %></td>
  <td><%= link_to "Eliminar", post_path(post), data: { method: "DELETE" } %></td>
</tr>
